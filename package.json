{
  "name": "bbh-chatbox-hybrid-chat",
  "version": "1.0.42",
  "private": true,
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev": "vite --mode dev",
    "staging": "vite --mode staging",
    "prod": "vite --mode prod",
    "build-dev": "run-p type-check build-only-dev",
    "build-staging": "run-p type-check build-only-staging",
    "build-prod": "run-p type-check build-only-prod",
    "build": "run-p type-check build-only-prod",
    "build-sub-path": "VITE_BASE_URL=/chat run-s build zip",
    "build-only-dev": "vite build --mode dev",
    "build-only-staging": "vite build --mode staging",
    "build-only-prod": "vite build --mode prod",
    "type-check": "vue-tsc --noEmit",
    "zip": "zip -r dist.zip dist"
  },
  "dependencies": {
    "@chenfengyuan/vue-qrcode": "^2.0.0",
    "@heroicons/vue": "^2.2.0",
    "@lottiefiles/dotlottie-vue": "^0.7.0",
    "@vueuse/core": "^12.4.0",
    "async": "^3.2.4",
    "autoprefixer": "^10.4.13",
    "date-fns": "2.30.0",
    "dayjs": "^1.11.13",
    "dompurify": "^3.2.3",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "marked": "^15.0.12",
    "pinia": "^2.0.28",
    "postcss": "^8.4.19",
    "query-string": "^8.1.0",
    "reflect-metadata": "^0.2.2",
    "sass": "^1.56.2",
    "sweetalert2": "^11.7.12",
    "tailwindcss": "^3.2.4",
    "tinykeys": "^3.0.0",
    "tsyringe": "^4.8.0",
    "validator": "^13.12.0",
    "vue": "^3.4.27",
    "vue-cleave-component": "^3.0.2",
    "vue-datepicker-next": "^1.0.2",
    "vue-i18n": "9.13.1",
    "vue-router": "4.3.3",
    "vue-toastification": "2.0.0-rc.5",
    "vue-virtual-scroller": "2.0.0-beta.8",
    "vue3-virtual-scroll-list": "^0.2.1",
    "vuedraggable": "^4.1.0",
    "yup": "^1.6.1",
    "zca-js": "2.0.0-beta.8"
  },
  "devDependencies": {
    "@types/async": "^3.2.16",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.16.18",
    "@types/validator": "^13.12.2",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/tsconfig": "^0.1.3",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vue-tsc": "^1.0.12"
  }
}
